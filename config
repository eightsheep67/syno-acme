# 你主域名，如 baidu.com sina.com.cn 等
export DOMAIN=your_domain

# DNS类型，根据域名服务商而定
export DNS=dns_dp

# DNS API 生效等待时间 值(单位：秒)
# 某些域名服务商的API生效时间较大，需要将这个值加大(比如900)
export DNS_SLEEP=120

# 阿里云 DNS=dns_ali
export Ali_Key="1234"
export Ali_Secret="asdf"

# Dnspod DNS=dns_dp
export DP_Id="1234"
export DP_Key="asdf"

# Godaddy DNS=dns_gd
export GD_Key="1234"
export GD_Secret="asdf"

# AWS DNS=dns_aws
export AWS_ACCESS_KEY_ID="1234"
export AWS_SECRET_ACCESS_KEY="asdff"

# Linode DNS=dns_linode
export LINODE_API_KEY="xxxxxxxx"
